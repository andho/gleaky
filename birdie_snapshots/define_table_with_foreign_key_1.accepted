Table(
  "addresses",
  [
    IntColumn(
      Address(AddressId),
      "id",
      ColumnConstraint(
        NotNull,
        NoForeignKey,
        NoDefault,
      ),
    ),
    StringColumn(
      Address(Street),
      "street",
      ColumnConstraint(
        NotNull,
        NoForeignKey,
        Default(StringValue("Majeedhee Magu")),
      ),
    ),
    IntColumn(
      Address(City),
      "city",
      ColumnConstraint(
        Null,
        NoForeignKey,
        NoDefault,
      ),
    ),
    IntColumn(
      Address(AddressCustomer),
      "customer_id",
      ColumnConstraint(
        NotNull,
        ForeignKey(
          [Customer(CustomerId)],
          OnDelete(Cascade),
          OnUpdate(Cascade),
        ),
        NoDefault,
      ),
    ),
  ],
)